        app:layout_constraintStart_toEndOf="@+id/textLargeur"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/spinnerGagner"
        android:layout_width="75dp"
        android:layout_height="20dp"
        android:layout_marginEnd="128dp"
